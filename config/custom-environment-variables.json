{
  "openapiConfig": {
    "filePath": "OPENAPI_FILE_PATH"
  },
  "telemetry": {
    "serviceName": "TELEMETRY_SERVICE_NAME",
    "hostname": "TELEMETRY_HOST_NAME",
    "version": "TELEMETRY_SERVICE_VERSION",
    "logger": {
      "level": "LOG_LEVEL",
      "prettyPrint": {
        "__name": "LOG_PRETTY_PRINT_ENABLED",
        "__format": "boolean"
      }
    },
    "tracing": {
      "enabled": "TELEMETRY_TRACING_ENABLED",
      "url": "TELEMETRY_TRACING_URL"
    },
    "metrics": {
      "enabled": "TELEMETRY_METRICS_ENABLED",
      "url": "TELEMETRY_METRICS_URL",
      "interval": "TELEMETRY_METRICS_INTERVAL"
    }
  },
  "server": {
    "port": "SERVER_PORT",
    "request": {
      "payload": {
        "limit": "REQUEST_PAYLOAD_LIMIT"
      }
    },
    "response": {
      "compression": {
        "enabled": {
          "__name": "RESPONSE_COMPRESSION_ENABLED",
          "__format": "boolean"
        }
      }
    }
  },
  "configProvider": "CONFIG_PROVIDER",
  "updatedTimeJsonFilePath": "FS_UPDATED_TIME_JSON_FILE_PATH",
  "poll": {
    "timeout": {
      "frequencyMilliseconds": "POLL_TIMEOUT_FREQUENCY_MS",
      "readinessKillMaxRandomSeconds": "POLL_TIMEOUT_READINESS_KILL_MAX_RND_SECONDS",
      "livenessKillSeconds": "POLL_TIMEOUT_LIVENESS_KILL_SECONDS"
    }
  },
  "FS": {
    "yamlFilePath": "FS_YAML_FILE_PATH"
  },
  "S3": {
    "accessKeyId": "S3_ACCESS_KEY_ID",
    "secretAccessKey": "S3_SECRET_ACCESS_KEY",
    "endpointUrl": "S3_ENDPOINT_URL",
    "bucket": "S3_BUCKET",
    "sslEnabled": {
      "__name": "S3_SSL_ENABLED",
      "__format": "boolean"
    }
  },
  "DB": {
    "host": "DB_HOST",
    "port": "DB_PORT",
    "user": "DB_USER",
    "password": "DB_PASSWORD",
    "database": "DB_NAME",
    "enableSslAuth": "DB_SSL_ENABLE",
    "rejectUnauthorized": "DB_SSL_REJECT_UNAUTHORIZED",
    "sslPaths": {
      "ca": "DB_SSL_CA",
      "key": "DB_SSL_KEY",
      "cert": "DB_SSL_CERT"
    }
  }
}
