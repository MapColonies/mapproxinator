{
  "openapiConfig": {
    "filePath": "./openapi3.yaml",
    "basePath": "/docs",
    "rawPath": "/api",
    "uiPath": "/api"
  },
  "telemetry": {
    "logger": {
      "level": "info",
      "prettyPrint": false
    }
  },
  "server": {
    "port": "8080",
    "request": {
      "payload": {
        "limit": "1mb"
      }
    },
    "response": {
      "compression": {
        "enabled": true,
        "options": null
      }
    }
  },
  "initMode": true,
  "configProvider": "fs",
  "updatedTimeFileName": "updated_time.json",
  "updatedTimeJsonFilePath": "/path/to/existing/updated_time.json",
  "yamlDestinationFilePath": "/path/to/destination/mapproxy.yaml",
  "poll": {
    "timeout": {
      "frequencyMilliseconds": 5000,
      "readinessKillMaxRandomSeconds": 300,
      "requestsKillSeconds": 5,
      "afterUpdateDelaySeconds": 0.5
    }
  },
  "FS": {
    "yamlSourceFilePath": "/path/to/source/mapproxy.yaml"
  },
  "S3": {
    "accessKeyId": "minioadmin",
    "secretAccessKey": "minioadmin",
    "endpointUrl": "http://localhost:9000",
    "bucket": "",
    "objectKey": "mapproxy.yaml",
    "sslEnabled": false
  },
  "DB": {
    "host": "localhost",
    "user": "postgres",
    "password": "postgres",
    "database": "mapproxy-config-dev",
    "port": 5432,
    "table": "config",
    "columns": {
      "data": "data",
      "updatedTime": "updated_time"
    },
    "enableSslAuth": false,
    "rejectUnauthorized": true,
    "sslPaths": {
      "ca": "",
      "key": "",
      "cert": ""
    }
  }
}
